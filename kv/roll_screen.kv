<Chicago_Roll>:
    Screen:
        id: Chicago_Roll
        name: 'Chicago_Roll'
        BoxLayout:
            orientation: 'vertical'
            ActionBar:
                background_color: (0, 5, 0, 0.8)
                pos_hint: {'top':1}
                ActionView:
                    use_sperator: True
                    ActionPrevious: 
                        app_icon: 'Images/txt.png'
                        with_previous: True
                        on_release: 
                           
                            root.manager.current = 'Menu'
                    ActionOverflow:
                    ActionButton:
                        icon: 'ico.png'
                        on_release: root.manager.current = 'Cart'
                            
            ScrollView:
                GridLayout:
                    cols: 2
                    padding: 3
                    spacing: 3
                    id: chicago_container